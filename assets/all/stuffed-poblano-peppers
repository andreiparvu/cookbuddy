!"], "images": [{"url": "http://lh6.ggpht.com/_OaYG005JPDs/S4rTEwsU1DI/AAAAAAAAA7U/2fI3B4S0DCQ/s640/Stuffed%20Poblanos%202.jpg", "path": "full/1e05baac41d9ce1c62e29722bf21ea39e07aa187.jpg", "checksum": "63da9d56dad9b9166748bea583407f99"}], "link": "http://www.budgetbytes.com/2010/02/stuffed-poblano-peppers/", "recipe_cook_time": ["PT1H30M"], "recipe_yield": ["6"]}